# Environment Configuration Template
# Copy this file to .env and fill in your actual API keys and configuration values

# Alpaca Markets API Configuration
# Get these from: https://alpaca.markets/
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets
# For live trading, use: https://api.alpaca.markets

# Supabase Configuration
# Get these from your Supabase project dashboard
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth Configuration
# Get these from: https://console.developers.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple OAuth Configuration
# Get these from: https://developer.apple.com/
APPLE_SERVICE_ID=your_apple_service_id_here
APPLE_TEAM_ID=your_apple_team_id_here
APPLE_KEY_ID=your_apple_key_id_here

# App Environment
APP_ENV=development
# Options: development, staging, production

# Firebase Configuration (if using Firebase for push notifications)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key

# WebSocket Configuration
ALPACA_WS_URL=wss://stream.data.alpaca.markets/v2/iex
# For live data, use: wss://stream.data.alpaca.markets/v2/sip

# Additional API Keys (for future integrations)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
POLYGON_API_KEY=your_polygon_api_key_here